{
  "name": "frst-components",
  "version": "0.1.7",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && mkdirp dist && rollup --config rollup.config.js",
    "sb": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@mui/icons-material": "^5.1.0",
    "@mui/material": "^5.4.0",
    "@types/styled-components": "^5.1.21",
    "i18next": "^21.6.10",
    "polished": "^4.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.15.3",
    "react-scripts": "^5.0.0",
    "styled-components": "^5.3.3",
    "web-vitals": "^1.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "**/trim": "^1.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.4",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@storybook/addon-actions": "^6.5.0-alpha.34",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.5.0-alpha.34",
    "@storybook/builder-webpack5": "^6.5.0-alpha.34",
    "@storybook/manager-webpack5": "^6.5.0-alpha.34",
    "@storybook/node-logger": "^6.5.0-alpha.34",
    "@storybook/react": "^6.1.21",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.38",
    "babel-plugin-styled-components": "^2.0.2",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.0",
    "rollup-plugin-img": "^1.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "storybook-react-i18next": "^1.0.17",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0"
  }
}
